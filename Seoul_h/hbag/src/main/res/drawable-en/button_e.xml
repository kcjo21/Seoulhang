<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" android:drawable="@color/Carkviewline" />
    <item android:state_focused="true" android:drawable="@color/Carkviewline" />

    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dp"
                android:color="@color/Carkviewline"/>
            <solid android:color="@color/Green00cc66" />
            <corners android:radius="5dp"/>
        </shape>
    </item>
</selector>